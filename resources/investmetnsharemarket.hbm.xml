<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.track365.hibernate.InvestmentShareMarket" table="investment_sm">

		<id name="Id" column="id">
			<generator class="increment">
			</generator>
		</id>

		<property name="UserId">
			
			<column name="user_id"></column>
		</property>
	
		<property name="StockExchange">
			<column name="stock_exchange"></column>
		</property>
		
		<property name="CompanyName">
			<column name="company_name"></column>
		</property>
		
		<property name="CompanyCode">
			<column name="company_code"></column>
		</property>
		
		<property name="BuyDate">
			<column name="buy_date"></column>
		</property>
		
		<property name="Units">
			<column name="units"></column>
		</property>
		
		<property name="UnitPrice">
			<column name="unit_price"></column>
		</property>
		
		<property name="Description">
			<column name="description"></column>
		</property>
			
	</class>

</hibernate-mapping>
