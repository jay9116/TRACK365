<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC
  		"-//OpenSymphony Group//XWork Validator 1.0.2//EN"
  		"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>

	
	<field name="SchemeName">
		<field-validator type="requiredstring">
			<message>Enter Scheme name</message>
		</field-validator>
	</field>
	
	 
	<field name="SchemeCode">
		<field-validator type="requiredstring">
			<message>Enter scheme code</message>
		</field-validator>
	</field>
	
	<field name="PlanType">
		<field-validator type="requiredstring">
			<message>Enter Plan Type</message>
		</field-validator>
	</field>
	
	<field name="Date">
		<field-validator type="date">
			<message>Select Date</message>
		</field-validator>
	</field>

</validators>